<?xml version="1.0" standalone="no"?>
<unit>
  <parameters>
    <armor-type value="void"/>
    <time value="25"/>
    <multi-selection value="true"/>
    <cellmap value="false"/>
    <fields>
      <field value="land"/>
    </fields>
    <properties>
      <property value="burnable"/>
    </properties>
    <tags>
      <tag value="building"/>
      <tag value="guildhall"/>
      <tag value="faction"/>
    </tags>
    <meeting-point value="false"/>
  </parameters>
  <item-limit limit="60"/>
  <resource-stores>
    <resource status="stockpile" type="wealth" stored="2000"/>
    <resource status="good" type="logs" stored="250"/>
    <resource status="good" type="stone" stored="100"/>
  </resource-stores>
  <creatable-type>
    <size value="5"/>
    <height value="2"/>
    <statistics>
      <enhancement>
	<resource-pools>
	  <max-hp value="2000"/>
	</resource-pools>
	<unit-stats>
	  <sight value="10" value-mult="0"/>
	</unit-stats>
      </enhancement>
    </statistics>
    <producible>
      <time value="10"/>
      <properties/>
      <light enabled="false"/>
      <unit-requirements/>
      <upgrade-requirements/>
      <resource-requirements>
        <resource name="wealth" amount="2000" plus="0" multiply="0"/>
        <resource name="population" amount="10" plus="0" multiply="0"/>
        <resource name="logs" amount="50" plus="0" multiply="0"/>
        <resource name="stone" amount="50" plus="0" multiply="0"/>
      </resource-requirements>
      <image path="images/castle.bmp"/>
      <image-cancel path="../ranger/images/tech_cancel.bmp"/>
      </producible>
    <resource-production>
      <resources-stored>
        <resource name="wealth" amount="100000"/>
        <resource name="logs" amount="100000"/>
        <resource name="stone" amount="100000"/>
      </resources-stored>
      <starter-resources>
        <resource name="wealth" amount="1000"/>
      </starter-resources>
    </resource-production>
    <units-owned>
      <unit name="hauler" limit="2"/>
    </units-owned>
    <actions>
      <skills>
        <skill>
          <type value="stop" />
          <name value="stop_skill"/>		
          <ep-cost value="0" />
          <speed value="300" />
	  <sounds-animations>
            <anim-speed value="100" />
            <animation path="models/eastern_workshop.g3d" />
            <particles value="false"/>
            <sound enabled="false" />
	  </sounds-animations>
        </skill>
        <skill>
          <type value="be_built" />
          <name value="be_built_skill"/>		
          <ep-cost value="0" />
          <speed value="300" />
	  <sounds-animations>
            <anim-speed value="300" />
            <animation path="models/eastern_workshop_build.g3d" />
            <sound enabled="false" />
	  </sounds-animations>
        </skill>
        <skill> 
          <type value="load" /> 
          <name value="load_skill" /> 
          <ep-cost value="0" /> 
          <speed value="120" />
	  <sounds-animations> 
            <anim-speed value="120" /> 
            <animation path="models/eastern_workshop.g3d" /> 
            <sound enabled="false"/>
	  </sounds-animations>
          <max-range value="3" /> 
          <particles value="false"/>
        </skill>
        <skill>
          <type value="unload"/>
          <name value="unload_skill"/>
          <ep-cost value="0"/>
          <speed value="120"/>
	  <sounds-animations>
            <anim-speed value="120"/>
            <animation path="models/eastern_workshop.g3d"/>
            <sound enabled="false"/>
	  </sounds-animations>
          <max-range value="3"/>
        </skill>	
        <skill>
          <type value="produce"/>
          <name value="produce_skill"/>
          <ep-cost value="0"/>
          <speed value="300"/>
	  <sounds-animations>
            <anim-speed value="100"/>
            <animation path="models/eastern_workshop.g3d"/>
            <particles value="false"/>
            <sound enabled="false"/>
	  </sounds-animations>
        </skill>		
        <skill>
          <type value="die" />
          <name value="die_skill"/>		
          <ep-cost value="0" />
          <speed value="300" />
	  <sounds-animations>
            <anim-speed value="300" />
            <animation path="models/eastern_farm_die.g3d" />
            <particles value="false"/>
            <sound enabled="false"/>
	  </sounds-animations>	
          <fade value="false"/>
        </skill>
        <skill>
          <type value="build"/>
          <name value="build_skill"/>
          <ep-cost value="0"/>
          <speed value="500"/>
	  <sounds-animations>
            <anim-speed value="100"/>
            <animation path="models/eastern_workshop.g3d"/>
            <sound enabled="false"/>
	  </sounds-animations>
        </skill>
      </skills>
      <commands>
        <command>
          <type value="structure"/>
          <name value="build_wood_cutters_hut"/>
          <image path="../wood_cutters_hut/images/mossy_shack.bmp"/>
          <unit-requirements/>
          <upgrade-requirements/>
          <build-skill value="build_skill"/>
          <buildings>
            <building name="wood_cutters_hut" tip="wood_cutters_hut_info"/>
          </buildings>
          <start-sound enabled="false"/>
          <built-sound enabled="false"/>
        </command>
        <command>
          <type value="structure"/>
          <name value="build_stone_cutters_shed"/>
          <image path="../stone_cutters_shed/images/warriors_hall.bmp"/>
          <unit-requirements/>
          <upgrade-requirements/>
          <build-skill value="build_skill"/>
          <buildings>
            <building name="stone_cutters_shed" tip="stone_cutters_shed_info"/>
          </buildings>
          <start-sound enabled="false"/>
          <built-sound enabled="false"/>
        </command>
        <command>
          <type value="produce"/>
          <name value="produce_hauler" tip="hauler_info"/>
          <image path="../hauler/images/worker.bmp"/>
          <unit-requirements />
          <upgrade-requirements />
          <produce-skill value="produce_skill"/>
          <produced-unit name="hauler"/>
        </command>
        <command>
          <type value="load"/>
          <name value="load"/>
          <image path="images/castle.bmp"/>
          <unit-requirements/>
          <upgrade-requirements/>
          <load-skill value="load_skill"/>
          <units-carried>
            <unit value="hauler"/>
          </units-carried>
          <load-capacity value="12" />
          <allow-projectiles value="false"/> 
        </command>
        <command>
          <type value="unload"/>
          <name value="unload"/>
          <image path="images/castle.bmp"/>
          <unit-requirements/>
          <upgrade-requirements/>
          <unload-skill value="unload_skill"/>
        </command>
      </commands>
    </actions>
  </creatable-type>
    <selection-sounds enabled="true">
      <sound path="sounds/tech_click.wav" />
    </selection-sounds>
    <command-sounds enabled="true">
      <sound path="sounds/tech_click.wav" />
    </command-sounds>
</unit>